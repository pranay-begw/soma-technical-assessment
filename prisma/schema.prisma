generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Todo {
  id            Int       @id @default(autoincrement())
  title         String
  description   String
  dueDate       DateTime
  createdAt     DateTime  @default(now())
  imageUrl      String?
  dependencies  String    @default("[]")
}